{"ast":null,"code":"// src/hooks/useAuth.js\nimport{useEffect,useState}from\"react\";import{onAuthStateChanged}from\"firebase/auth\";import{auth}from\"../firebase/firebase\";export function useAuth(){const[user,setUser]=useState(null);useEffect(()=>{const unsub=onAuthStateChanged(auth,firebaseUser=>{setUser(firebaseUser);});return()=>unsub();},[]);return user;}","map":{"version":3,"names":["useEffect","useState","onAuthStateChanged","auth","useAuth","user","setUser","unsub","firebaseUser"],"sources":["/Users/andrijzuk/AndriZhok.github.io/lab4/bookstore/src/hooks/useAuth.js"],"sourcesContent":["// src/hooks/useAuth.js\nimport { useEffect, useState } from \"react\";\nimport { onAuthStateChanged } from \"firebase/auth\";\nimport { auth } from \"../firebase/firebase\";\n\nexport function useAuth() {\n  const [user, setUser] = useState(null);\n\n  useEffect(() => {\n    const unsub = onAuthStateChanged(auth, (firebaseUser) => {\n      setUser(firebaseUser);\n    });\n    return () => unsub();\n  }, []);\n\n  return user;\n}"],"mappings":"AAAA;AACA,OAASA,SAAS,CAAEC,QAAQ,KAAQ,OAAO,CAC3C,OAASC,kBAAkB,KAAQ,eAAe,CAClD,OAASC,IAAI,KAAQ,sBAAsB,CAE3C,MAAO,SAAS,CAAAC,OAAOA,CAAA,CAAG,CACxB,KAAM,CAACC,IAAI,CAAEC,OAAO,CAAC,CAAGL,QAAQ,CAAC,IAAI,CAAC,CAEtCD,SAAS,CAAC,IAAM,CACd,KAAM,CAAAO,KAAK,CAAGL,kBAAkB,CAACC,IAAI,CAAGK,YAAY,EAAK,CACvDF,OAAO,CAACE,YAAY,CAAC,CACvB,CAAC,CAAC,CACF,MAAO,IAAMD,KAAK,CAAC,CAAC,CACtB,CAAC,CAAE,EAAE,CAAC,CAEN,MAAO,CAAAF,IAAI,CACb","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}